<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="700.0" prefWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.logic.Controller.LoginControllerGUI">
   <children>
      <Label layoutX="194.0" layoutY="93.0" text="BENVENUTO LUCA!">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label layoutX="189.0" layoutY="197.0" text="Per favore, inserisci i tuoi dati.">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="username" alignment="CENTER" layoutX="211.0" layoutY="289.0" promptText="username">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" alignment="CENTER" layoutX="211.0" layoutY="409.0" promptText="password">
         <font>
            <Font size="24.0" />
         </font>
      </PasswordField>
      <Button fx:id="autentificazione" layoutX="239.0" layoutY="558.0" mnemonicParsing="false" onAction="#autentificazione" text="AUTENTIFICAZIONE">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
