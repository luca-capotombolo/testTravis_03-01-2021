<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.logic.Controller.NotaPaginaControllerGUI">
   <children>
      <TableView fx:id="tabella" layoutX="112.0" layoutY="97.0" prefHeight="297.0" prefWidth="500.0" AnchorPane.leftAnchor="100.0">
        <columns>
          <TableColumn fx:id="noteColonna" minWidth="500.0" prefWidth="75.0" text="NOTE" />
        </columns>
      </TableView>
      <Label layoutX="243.0" layoutY="25.0" text="Inserire note">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <TextArea fx:id="areaTesto" layoutX="101.0" layoutY="411.0" prefHeight="200.0" prefWidth="500.0" promptText="Inserire testo..." />
      <Button fx:id="inserisci" layoutX="449.0" layoutY="626.0" mnemonicParsing="false" onAction="#inserisciNota" prefHeight="51.0" prefWidth="165.0" text="INSERT NOTE" />
      <Button fx:id="carica" layoutX="88.0" layoutY="626.0" mnemonicParsing="false" onAction="#caricaNote" prefHeight="51.0" prefWidth="165.0" text="LOAD NOTES" />
      <Button fx:id="homePage" layoutX="269.0" layoutY="626.0" mnemonicParsing="false" onAction="#caricaHomePage" prefHeight="51.0" prefWidth="165.0" text="HOME PAGE" />
   </children>
</AnchorPane>
