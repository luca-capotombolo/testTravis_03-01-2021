            <Font size="24.0" />
         </font>
      </Button>
      <TextField fx:id="esameCampo" alignment="CENTER" layoutX="100.0" layoutY="515.0" prefHeight="51.0" prefWidth="200.0" promptText="esame">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="votoCampo" alignment="CENTER" layoutX="400.0" layoutY="515.0" prefHeight="51.0" prefWidth="200.0" promptText="voto">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <Button fx:id="insert" layoutX="450.0" layoutY="618.0" mnemonicParsing="false" onAction="#inserisciDati" prefHeight="51.0" prefWidth="165.0" text="INSERT">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
