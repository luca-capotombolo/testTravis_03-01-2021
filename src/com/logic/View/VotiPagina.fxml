<AnchorPane fx:id="pane" prefHeight="700.0" prefWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.logic.Controller.VotiPaginaControllerGUI">
   <children>
      <TableView fx:id="tabella" layoutX="100.0" layoutY="95.0" prefHeight="388.0" prefWidth="500.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
        <columns>
          <TableColumn fx:id="colonnaEsame" minWidth="250.0" prefWidth="75.0" text="ESAME" />
          <TableColumn fx:id="colonnaVoto" minWidth="250.0" prefWidth="75.0" text="VOTO" />
        </columns>
      </TableView>
      <Label layoutX="71.0" layoutY="31.0" text="VOTI ESAMI LAUREA INGEGNERIA INFORMATICA">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="homePage" layoutX="90.0" layoutY="618.0" mnemonicParsing="false" onAction="#caricaHomePage" prefWidth="165.0" text="HOME PAGE">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="load" layoutX="269.0" layoutY="618.0" mnemonicParsing="false" onAction="#caricaVoti" prefHeight="51.0" prefWidth="165.0" text="LOAD">
